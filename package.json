{
  "name": "talberos",
  "version": "1.0.0",
  "description": "Talberos - Aplicación educativa desarrollada con Next.js, por Gabriel Hercules Miguel. Uso de las mejores prácticas para enseñanza y aprendizaje.",
  "author": "Gabriel Hercules Miguel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GabrielHerculesMiguel/Talberos.git"
  },
  "scripts": {
    "dev": "cross-env PORT=12000 NODE_OPTIONS=--max-old-space-size=2048 next dev",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next build",
    "postbuild": "next-sitemap",
    "start": "cross-env NODE_ENV=production PORT=12000 NODE_OPTIONS=--max-old-space-size=2048 next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "check-deps": "depcheck",
    "sitemap": "next-sitemap",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "jest --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@maxmind/geoip2-node": "^6.0.0",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.9",
    "@react-spring/web": "^9.7.5",
    "@tanstack/react-table": "^8.20.6",
    "@use-gesture/react": "^10.3.1",
    "axios": "^1.7.9",
    "cookie": "^1.0.2",
    "file-saver": "^2.0.5",
    "framer-motion": "^11.15.0",
    "googleapis": "^148.0.0",
    "gray-matter": "^4.0.3",
    "mysql": "^2.18.1",
    "mysql2": "^3.14.0",
    "next": "13.4.19",
    "next-mdx-remote": "^5.0.0",
    "prop-types": "latest",
    "react": "^18.3.1",
    "react-awesome-reveal": "^4.3.1",
    "react-dom": "^18.3.1",
    "react-draggable": "^4.4.6",
    "react-icons": "^5.4.0",
    "react-parallax-tilt": "^1.7.287",
    "react-select": "^5.10.0",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@lhci/cli": "^0.14.0",
    "@next/bundle-analyzer": "latest",
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@types/react": "^19.0.1",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "eslint-config-next": "latest",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "next-sitemap": "latest",
    "prettier": "^3.2.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "next lint --fix"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
